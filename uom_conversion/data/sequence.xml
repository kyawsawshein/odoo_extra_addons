<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Sequences from tracking numbers -->
        <record id="sequence_uom_converse" model="ir.sequence">
            <field name="name">UOM Conversion</field>
            <field name="code">uom.conversion.serial</field>
            <field name="prefix">UOM%(y)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="use_date_range">True</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
