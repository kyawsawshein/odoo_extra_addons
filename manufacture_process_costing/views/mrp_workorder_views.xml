<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_mrp_production_workorder_form_view_filter_default_search" model="ir.ui.view">
        <field name="name">mrp.production.work.order.select</field>
        <field name="model">mrp.workorder</field>
        <field name="inherit_id" ref="mrp.view_mrp_production_workorder_form_view_filter"/>
        <field name="arch" type="xml">
            <xpath expr="//search/field[@name='production_id']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>
            <xpath expr="//search/field[@name='name']" position="before">
                <field name="production_id"/>
            </xpath>
        </field>
    </record>

</odoo>
